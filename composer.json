{
    "name": "ts2201/vuepagereview",
    "description": "Add Vue Page Comment in laravel app",
    "license": "MIT",
    "authors": [
        {
            "name": "Nguyen The Bien",
            "email": "thebienpronguyen2201@gmail.com",
            "homepage": "author homepage"
        }
    ],
    "homepage": "https://github.com/ts2201/vuepagereview",
    "keywords": ["Laravel", "VuePageReview"],
    "require": {
        "illuminate/support": "~5|~6|~7"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "TS2201\\VuePageReview\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TS2201\\VuePageReview\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TS2201\\VuePageReview\\PageReviewServiceProvider"
            ],
            "aliases": {
                "VuePageReview": "TS2201\\VuePageReview\\Facades\\VuePageReview"
            }
        }
    }
}
