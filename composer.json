{
    "name": "laramore/pagereview",
    "description": "Add page review in laravel app",
    "license": "MIT",
    "authors": [
        {
            "name": "Nguyen The Bien",
            "email": "thebienpronguyen2201@gmail.com",
            "homepage": "author homepage"
        }
    ],
    "homepage": "https://github.com/laramore/pagereview",
    "keywords": ["Laravel", "PageReview"],
    "require": {
        "illuminate/support": "~5|~6|~7"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Laramore\\PageReview\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laramore\\PageReview\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laramore\\PageReview\\PageReviewServiceProvider"
            ],
            "aliases": {
                "PageReview": "Laramore\\PageReview\\Facades\\PageReview"
            }
        }
    }
}
